{
    "manifest_version": 3,
    "name": "GOAWAYLOGOUT",
    "version": "1.0",
    "description": "Toggle to hide the logout button on YouTube.",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
